{
  "name": "fast-sudoku-solver",
  "version": "2.0.0",
  "description": "A fast Sudoku solver written in TypeScript.",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --fix --max-warnings 0",
    "format": "prettier --write \"./**/*.{js,json,md,ts,yaml,yml}\"",
    "test": "jest",
    "clean": "rimraf dist",
    "build": "tsc",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luth1um/sudoku-solver-typescript"
  },
  "keywords": [
    "sudoku",
    "puzzle",
    "solver"
  ],
  "author": "luthium",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@types/jest": "30.0.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "lint-staged": "16.2.7",
    "prettier": "3.7.3",
    "rimraf": "6.1.2",
    "ts-jest": "29.4.5",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix --max-warnings 0 --no-warn-ignored"
    ],
    "*.{json,md,yaml,yml}": "prettier --write"
  }
}
